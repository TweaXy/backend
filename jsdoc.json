{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": ["."],
        "exclude": ["node_modules", "src/routes"]
    },
    "recurseDepth": 10,
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "opts": {
        "destination": "./docs/function/",
        "recurse": true,
        "template": "./node_modules/tui-jsdoc-template"
    },
    "templates": {
        "logo": {
            "url": "https://avatars.githubusercontent.com/u/147094143?s=200&v=4",
            "width": "90px",
            "height": "90px",
            "link": "https://github.com/TweaXy/backend"
        },
        "name": "TweaXy Backend",
        "footerText": "My awesome function docs!"
    }
}
